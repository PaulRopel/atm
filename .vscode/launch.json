{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug SvelteKit",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "outFiles": ["${workspaceFolder}/src/web/.svelte-kit/**/*.js"],
      "sourceMaps": true,
      "cwd": "${workspaceFolder}/src/web",
      "envFile": "${workspaceFolder}/src/web/.env"
    }
  ]
}
