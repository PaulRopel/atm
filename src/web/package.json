{
	"name": "web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "cross-env NODE_ENV=development npm run dev:node",
		"dev:static": "cross-env NODE_ENV=development VITE_ADAPTER=static vite dev",
		"dev:node": "cross-env NODE_ENV=development VITE_ADAPTER=node vite dev --port 5173",
		"build": "vite build",
		"build:node": "cross-env NODE_ENV=production VITE_ADAPTER=node vite build",
		"build:static": "cross-env NODE_ENV=production VITE_ADAPTER=static vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"preview": "cross-env NODE_ENV=production npm run preview:node",
		"preview:static": "cross-env NODE_ENV=production npm run build:static && VITE_ADAPTER=static vite preview",
		"preview:node": "cross-env NODE_ENV=production npm run build:node && VITE_ADAPTER=node vite preview --port 3000",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"test:unit": "vitest",
		"capacitor:copy:before": "cross-env NODE_ENV=production npm run build:static",
		"capacitor": "capacitor",
		"cap:sync": "npm run capacitor:copy:before && npx cap sync",
		"cap:build": "npm run capacitor:copy:before && npx cap build",
		"cap:open": "npm run capacitor:copy:before && npx cap open",
		"android": "npm run cap:sync && npm run cap:open android"
	},
	"devDependencies": {
		"@capacitor/android": "^6.2.0",
		"@capacitor/cli": "^6.2.0",
		"@capacitor/core": "^6.2.0",
		"@capacitor/ios": "^6.2.0",
		"@eslint/compat": "^1.2.3",
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-node": "^5.2.11",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/node": "^22.10.5",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.0.0-next.77",
		"clsx": "^2.1.1",
		"cross-env": "^7.0.3",
		"drizzle-kit": "^0.22.0",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"formsnap": "^2.0.0",
		"globals": "^15.0.0",
		"lucide-svelte": "^0.469.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"sveltekit-superforms": "^2.22.1",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.3.0",
		"tailwindcss": "^3.4.9",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.4.11",
		"vitest": "^2.0.4",
		"zod": "^3.24.1"
	},
	"dependencies": {
		"@inlang/paraglide-sveltekit": "^0.11.5",
		"@libsql/client": "^0.9.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@types/dotenv": "^6.1.1",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.33.0",
		"mode-watcher": "^0.5.0",
		"sst": "3.5.7",
		"svelte-kit-sst": "2.43.5"
	}
}
